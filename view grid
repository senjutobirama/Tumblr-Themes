<div id="toTop" title="back to top">
{block:ifNotScrollUpTxt}
<img src="{image:scrollup}" style="display: block;" title="back to top">
{/block:ifNotScrollUpTxt}
{block:ifScrollUpTxt}
{text:scroll up txt}
{/block:ifScrollUpTxt}
</div>
    
<script>
    $(window).scroll(function() {
  if ($(this).scrollTop()) {
    $('#toTop').fadeIn();
  } else {
    $('#toTop').fadeOut();
  }
});

$("#toTop").click(function() {
  $("html, body").animate({
    scrollTop: 0
  }, 1000);
});
</script>


