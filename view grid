  <script>
     var $container = $('main');
   $container.find('.notecount').each(function(){
       var n = $(this).html().split(' ')[0].replace(/,/g, '');
       if (n > 999) {
           n = Math.floor(n / 100) / 10;
           $(this).text(n + 'k {block:PermalinkPage}notes{/block:PermalinkPage}');
       }
   });
</script>


